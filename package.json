    "base64-arraybuffer": "^0.2.0",
    "bluebird": "^3.5.1",
    "card-validator": "^8.1.0",
    "commerce-sdk-isomorphic": "1.1.2",
    "core-js": "2.4.0",
    "cross-fetch": "^3.1.5",
    "express": "^4.16.4",
    "focus-visible": "^5.2.0",
    "framer-motion": "^3.7.0",
    "helmet": "^4.6.0",
    "js-cookie": "^3.0.1",
    "jwt-decode": "^3.1.2",
    "nanoid": "^3.1.31",
    "prop-types": "^15.6.0",
    "pwa-kit-dev": "^2.0.0",
    "pwa-kit-react-sdk": "^2.0.0",
